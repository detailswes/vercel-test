{
    "version": 2,
    "buildCommand": "yarn build",
    "framework": "express",
    "installCommand": "yarn install",
    "outputDirectory": "build",
    "builds": [
      {
        "src": "servers.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "servers.js"
      }
    ]
  }